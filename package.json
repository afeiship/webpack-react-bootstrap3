    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.3.1",
    "img-loader": "^2.0.0",
    "import-glob-loader": "^1.1.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^1.3.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
