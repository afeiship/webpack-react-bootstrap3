{
  "name": "webpack-react-bootstrap3",
  "version": "1.0.0",
  "description": "Webpack for bootstrap3 & react.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dll-dev": "webpack --config ./build/webpack.config.dll-dev.babel.js",
    "dll-prd": "webpack --config ./build/webpack.config.dll-prd.babel.js",
    "predev": "npm run clean && npm run dll-dev",
    "prebuild": "npm run clean && npm run dll-prd",
    "build": "webpack -p --env=prd",
    "dev": "webpack-dev-server --env=dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afeiship/webpack-react-bootstrap3.git"
  },
  "keywords": [
    "webpack",
    "react",
    "bootstrap",
    "pc",
    "site",
    "website"
  ],
  "author": "afei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/afeiship/webpack-react-bootstrap3/issues"
  },
  "homepage": "https://github.com/afeiship/webpack-react-bootstrap3#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "mixin-decorator": "github:afeiship/mixin-decorator",
    "next-date": "github:afeiship/next-date",
    "next-js-core2": "github:afeiship/next-js-core2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "webkit-sass-reset": "github:afeiship/webkit-sass-reset"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.1.2",
    "autoprefixer": "^7.1.3",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-array-from": "^1.0.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-2": "^6.24.1",
